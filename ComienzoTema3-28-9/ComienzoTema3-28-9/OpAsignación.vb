Module OpAsignación
    Sub main()
        'operadores de asignación: =, += , -= , *= , /=

        Dim x As Integer = 1
        'sumar 1 a x --->   x = x + 1
        x += 1
        Console.WriteLine("x = " & x)


        'restar 1 a x --->   x = x - 1
        x -= 1
        Console.WriteLine("x = " & x)


    End Sub
End Module
